                                        <div data-type="helo" data-MFD="{!obj.MFD}"  title="" id="{!indx}" onclick="{!c.get_loanFor_margin}" style="cursor:pointer;text-align:center;">                                                 
                                            <button id="{!indx}" data-type="helo" data-MFD="{!obj.MFD}" style="color:white; background-color:#13547c" class="slds-button slds-button--inverse " onclick="{!c.get_loanFor_margin}" >Select</button>                            
                                        </div>
                                    </th><!--Bala -->
                                    <th scope="row" data-MFD="{!obj.MFD}"  data-label="Margin" data-type="helo" class="tbody_td" style="    background-color: #2081bf;">
                                        <div data-type="helo" data-MFD="{!obj.MFD}" class="slds-truncate" title="" id="{!indx}" onclick="{!c.get_loanFor_margin}" style="cursor:pointer;text-align:center;">                                                 
                                            <span data-type="helo" data-MFD="{!obj.MFD}" id="{!indx}" onclick="{!c.get_loanFor_margin}" style="color:white;" >{#obj.InterestRate} </span>
                                        </div>
                                    </th>
                                    <td data-label="" class="tbody_td" data-type="helo" data-MFD="{!obj.MFD}">
                                        <div data-MFD="{!obj.MFD}" data-type="helo" class="slds-truncate" title="" id="{!indx}" onclick="{!c.get_loanFor_margin}" style="cursor:pointer;text-align:center;">
                                            <span data-MFD="{!obj.MFD}" data-type="helo" id="{!indx}" onclick="{!c.get_loanFor_margin}" >
                                               <aura:if isTrue="{!obj.PrincipalLimit >= 4000000}"> <!--265 -->
                                                    $4,000,000.00
                                                    <aura:set attribute="else">
                                                        <lightning:formattedNumber value="{!obj.PrincipalLimit}" style="currency" />
                                                    </aura:set>
                                                </aura:if> 
                                            </span>
                                        </div>
                                    </td>
                                                                     
                                    <td data-label="" class="tbody_td" data-type="helo">
                                        <div data-MFD="{!obj.MFD}" data-type="helo" class="slds-truncate" title="" id="{!indx}" onclick="{!c.get_loanFor_margin}" style="cursor:pointer;text-align:center;">
                                            <span data-MFD="{!obj.MFD}" data-type="helo" id="{!indx}" onclick="{!c.get_loanFor_margin}" >                                                
                                                <aura:if isTrue="{! obj.CC >= 0}">
                                                    	<ui:outputCurrency value="{!obj.CC}" format=""  /> 
													<aura:set attribute="else">
                                                    	<div style="color:red;">
                                                    		- <ui:outputCurrency value="{!obj.CC}" format=""  />
                                                    	</div>
                                                    </aura:set>
                                                </aura:if>                                                
                                            </span>   
                                        </div>
                                    </td> 
                                    
                                    <td data-MFD="{!obj.MFD}" data-type="helo" data-label="" class="tbody_td">
                                        <div data-MFD="{!obj.MFD}"  data-type="helo" class="slds-truncate" title="" id="{!indx}" onclick="{!c.get_loanFor_margin}" style="cursor:pointer;text-align:center;">
                                            <span data-MFD="{!obj.MFD}" data-type="helo" id="{!indx}" onclick="{!c.get_loanFor_margin}" >    
                                                    <aura:if isTrue="{!obj.UPB >= 4000000}">  <!--265_new --> 
                                                            $4,000,000.00
                                                            <aura:set attribute="else">
                                                                <lightning:formattedNumber value="{!obj.UPB}" style="currency" />
                                                            </aura:set>
                                                    </aura:if>                                                                                           
                                            </span>
                                        </div>
                                    </td>
                                    <td data-MFD="{!obj.MFD}" data-type="helo" data-label="" class="tbody_td">
                                        <div data-MFD="{!obj.MFD}"  data-type="helo" class="slds-truncate" title=""  id="{!indx}" onclick="{!c.get_loanFor_margin}" style="cursor:pointer;text-align:center;">
                                            <span data-MFD="{!obj.MFD}" data-type="helo" id="{!indx}" onclick="{!c.get_loanFor_margin}" >                                               
                                                 
                                                <lightning:formattedNumber value="{!obj.SF}" style="currency" />  
                                            </span>
                                        </div>
                                    </td>
                                    <td data-MFD="{!obj.MFD}" data-type="helo" data-label="" class="tbody_td">
                                        <div data-MFD="{!obj.MFD}"  data-type="helo" class="slds-truncate" title="" id="{!indx}" onclick="{!c.get_loanFor_margin}" style="cursor:pointer;text-align:center;">  
                                            <span data-MFD="{!obj.MFD}" data-type="helo" id="{!indx}" onclick="{!c.get_loanFor_margin}" >
                                                <lightning:formattedNumber value="{!obj.TC}" style="currency" />
                                            </span>
                                        </div>
                                    </td>
                                </tr>
                            </aura:iteration>
                        </tbody>
                    </table>
                </div>
            </aura:if>
            <br/>
            <aura:if isTrue="{!v.Show_FHA_Hecm_FixedMargin}">
                <h4>HECM ARM</h4>
                <div style="">
                    <table class="margin_table slds-table slds-table_bordered slds-table_cell-buffer" style="border: 1px solid #d4d4d4;">
                        <thead class="margin-table-head" >
                            <tr class="slds-text-title_caps" style="text-align: left;background-color: #2081bf;color:white; ">
                                <th scope="row" class="tbody_td" 
                                    style="background-color: #2081bf;">                                    
                                </th> <!--Bala -->
                                
                                <th scope="row" data-label="Margin" class="tbody_td" 
                                    style="background-color: #2081bf;">
                                    <div class="slds-truncate customFontMargin" title="Margin" style="text-align:center;color: #fff;">
                                        Lender Margin
                                    </div>
                                </th>
                                <th scope="col" class="tabhead">
                                    <div class="slds-truncate customFont17" title="Principal Limit"
                                         style="text-align:center;">
                                        Principal Limit
                                        <lightning:helptext content="Total Amount Available" />
                                        
                                    </div>
                                </th>
                                <th scope="col" class="tabhead">
                                    <div class="slds-truncate customFont17" title="Cash At Close"  style="text-align:center;">
                                        {!v.typeOfFunds}
                                        <!-- added if condition - SRINI -->
                                        <aura:if isTrue="{! v.typeOfFunds == 'Cash at close'}">
                                            <lightning:helptext content="Maximum Cash Available at Close" />
                                            <aura:set attribute="else">
                                                <lightning:helptext content="This is the amount your client will need to bring to close" /><!--Maximum Cash Available at Close-->
                                            </aura:set>
                                        </aura:if>
                                        <!-- if condition ends- SRINI -->
                                    </div>
                                </th>
                                <aura:if isTrue="{!v.isTraditional}"> 
