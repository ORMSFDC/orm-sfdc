    <validationRules>
        <fullName>DailyProductRateOrigFeeValidRate2</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(Product_Rate_Type__c, &apos;HELO-OrigFee&apos;), 
OR(OR(Rate2__c &lt; 0.00000, ISBLANK(Rate2__c)), 
OR(Rate3__c &lt; 0.00000, ISBLANK(Rate3__c)), 
OR(Rate__c &lt; 0.00000, ISBLANK(Rate__c))) 
)</errorConditionFormula>
        <errorDisplayField>Product_Rate_Type__c</errorDisplayField>
        <errorMessage>HELO-Origination Fee REQUIRES a value in Rate, Rate2, and Rate3</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HELOBrokerCompValidRate</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(Product_Rate_Type__c, &apos;HELO-BrokerComp&apos;), 
OR(Rate__c &lt; 100, ISBLANK(Rate__c))
)</errorConditionFormula>
        <errorDisplayField>Rate__c</errorDisplayField>
        <errorMessage>Must be greater than 100</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HELOOrigFeeValidateRate2</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(Product_Rate_Type__c, &apos;HELO-OrigFee&apos;),
OR(
ISBLANK(Rate2__c),
NOT(Rate2__c&lt;0.01)
))</errorConditionFormula>
        <errorDisplayField>Rate2__c</errorDisplayField>
        <errorMessage>Must be a number between 1.0 and 0.0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HELOOrigFeeValidateRate3</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(Product_Rate_Type__c, &apos;HELO-OrigFee&apos;), 
OR( 
ISBLANK(Rate3__c), 
NOT(Rate3__c&lt;0.01) 
))</errorConditionFormula>
        <errorDisplayField>Rate3__c</errorDisplayField>
        <errorMessage>Must be a value between 1.0 and 0.0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NotOrigFeeValidateRate2</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ISPICKVAL(Product_Rate_Type__c, &apos;HELO-OrigFee&apos;)), 
NOT(ISBLANK(Rate2__c))
)</errorConditionFormula>
        <errorDisplayField>Rate2__c</errorDisplayField>
        <errorMessage>Rate 2 is not used for the selected Product Rate. Remove this value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NotOrigFeeValidateRate3</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ISPICKVAL(Product_Rate_Type__c, &apos;HELO-OrigFee&apos;)), 
NOT(ISBLANK(Rate3__c))
)</errorConditionFormula>
        <errorDisplayField>Rate3__c</errorDisplayField>
        <errorMessage>Rate 3 is not used for the selected Product Rate. Remove this value.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
