    <validationRules>
        <fullName>UW_on_Loan</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Loan__r.UW__c)</errorConditionFormula>
        <errorMessage>Please add an UW to this Loan</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UW_Cannot_Pick_Status_Set</fullName>
        <active>false</active>
        <description>UW is limited to status selections</description>
        <errorConditionFormula>AND( $UserRole.Name = 'Underwriter', ISCHANGED( Status__c ), 
OR( 
ISPICKVAL(Status__c, 'Ordered'),
ISPICKVAL(Status__c, 'Attempted'),
ISPICKVAL(Status__c, 'Received')
) 
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>You are not allowed to use this status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PCS_Cannot_Pick_Status_Set</fullName>
        <active>true</active>
        <description>PCS is unable to change status of UW Record Type</description>
        <errorConditionFormula>AND( $UserRole.Name = 'Partner Care Specialist', ISCHANGED( Status__c ), $RecordType.Name = 'UW',
OR( 
ISPICKVAL(Status__c, 'Condition Reviewed'),
ISPICKVAL(Status__c, 'Ordered'),
ISPICKVAL(Status__c, 'Attempted'),
ISPICKVAL(Status__c, 'Received'),
ISPICKVAL(Status__c, 'Opened')
) 
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>You are not allowed to change the status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sleep_checked_must_select_time_rame</fullName>
        <active>true</active>
        <description>If checkbox is selected, must select a time frame</description>
        <errorConditionFormula>AND( Sleep_TaskCheckbox__c  = TRUE, ISBLANK( TEXT( Sleep_Task__c) ))</errorConditionFormula>
        <errorDisplayField>Sleep_Task__c</errorDisplayField>
        <errorMessage>Must select a duration</errorMessage>
    </validationRules>
</CustomObject>
