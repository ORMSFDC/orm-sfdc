    <fields>
        <fullName>Task_Grouping__c</fullName>
        <externalId>false</externalId>
        <formula>IF(CONTAINS( Subject, 'Document has been uploaded'), 'Document Task', 
IF(CONTAINS( Subject, 'Counseling Expiration Date is approaching on loan ID'), 'Counseling Date Expiration Task', 
